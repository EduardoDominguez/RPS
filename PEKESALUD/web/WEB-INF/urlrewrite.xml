<urlrewrite>
    <rule>
        <from>/Login$</from>
        <to>login.htm</to>
    </rule>
    <rule>
        <from>/Logueo$</from>
        <to>login/ingresar.htm</to>
    </rule>
    <rule>
        <from>/Contenidos$</from>
        <to>content.htm</to>
    </rule>
    <rule>
        <from>/getInstitutions$</from>
        <to>login/getInstitutions.htm</to>
    </rule>
    <rule>
        <from>/([a-zA-z0-9]+)$</from>
        <to>index.htm</to>
    </rule>
</urlrewrite>
